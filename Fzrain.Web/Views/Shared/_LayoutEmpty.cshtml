<!DOCTYPE html>
<html>
<head>
   <title>@ViewBag.Title </title>
    @Html.Partial("ImportReferences")
    <style>
          ::-webkit-scrollbar {
            width: 10px;
        }

        ::-webkit-scrollbar-track,
        ::-webkit-scrollbar-thumb {
            border-radius: 999px;
        }

        ::-webkit-scrollbar-track {
            box-shadow: 1px 1px 5px rgba(0,0,0,.2) inset;
        }

        ::-webkit-scrollbar-thumb {
            box-shadow: 0 0 0 15px rgba(0,0,0,.2) inset;
        }

        ::-webkit-scrollbar-corner {
            background: transparent;
        }
    </style>
</head>

<body>
    @Html.Partial("Components/Notification")
    @Html.Partial("Components/Header")
  
    @RenderBody()

</body>
</html>
